fun sortNums(list) {
    for (i = 0; i < length(list); i = i + 1) {
        val = list[i];
        j = i;
        while (j > 0) {
            if (val < list[j]) {
                tmp = list[j];
                list[j] = val;
                val = tmp;
                j = j - 1;
            }
        }
    }

    return list;
}


fun main() {
    list = [9,8,7];
    list = sortNums(list);
}
